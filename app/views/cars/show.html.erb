<h1>詳細</h1>
    <div class="col-12 ml-auto">
        <table class="table table-hover">
            <tbody>
                <tr>
                  <th>車名</th>
                  <td><%=@car.car_name%></td>
                </tr>
                <tr>
                    <th>ナンバー</th>
                    <td><%=@car.number_plate%></td>
                </tr>
                <tr>
                    <th>ガス</th>
                    <td><% if @car.gas == 1 %> ⭕️ <% else %> ❌ <% end %></td>
                </tr>
                <tr>
                    <th>内装</th>
                    <td><% if @car.interior == 1 %> ⭕️ <% else %> ❌ <% end %></td>
                </tr>
                <tr>
                    <th>外装</th>
                    <td><% if @car.exterior == 1 %> ⭕️ <% else %> ❌ <% end %></td>
                </tr>
                <tr>
                    <th>CS</th>
                    <td><%=@car.c_sheet%></td>
                </tr>
                <tr>
                    <th>BS</th>
                    <td><%=@car.b_sheet%></td>
                </tr>
                <tr>
                    <th>JS</th>
                    <td><%=@car.j_sheet%></td>
                </tr>
            </tbody>
        </table>
    </div>
<%= link_to "編集", edit_car_path %>